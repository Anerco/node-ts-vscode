{
  "name": "node-ts-vscode",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "// ------ DEVELOPMENT ----- //": "",
    "debug": "nodemon src/index.ts",
    "convert": "ts-jest config:migrate ./jest.config.js",
    "// ------ TESTING ----- //": "",
    "test": "jest --watch",
    "coverage": "jest --collectCoverageFrom=src/**/*.ts --coverage",
    "// ------ BUILDING ----- //": "",
    "build": "tsc",
    "build:watch": "tsc -w",
    "// ------ LINTING ----- //": "",
    "lint:list": "eslint */**/*.js --fix",
    "lint": "eslint */**/*.js --fix",
    "// ------ PRODUCTION ----- //": "",
    "start:prod": "nodemon src/index.ts",
    "// ------ NPM ----- //": "",
    "prepublishOnly": "tsc"
  },
  "keywords": [
    "node.js",
    "vscode",
    "npm",
    "eslint",
    "prettier"
  ],
  "author": "Anerco",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.8",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^25.2.4",
    "prettier": "^1.16.4",
    "ts-jest": "^25.3.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3"
  }
}
